Algoritmo Ejercicio1
	cantidad<-5; 
	Dimension Almacen[cantidad]
	Para i<-1 Hasta cantidad Con Paso 1 Hacer
		Escribir "Ingrese un numero para la posición ",i 
		Leer Almacen[i]
	FinPara
	Escribir "Los numeros ingresados son : "
	Para i<-1 Hasta cantidad Con Paso 1 Hacer
		Escribir Almacen[i]
	FinPara
FinAlgoritmo

